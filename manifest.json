{
  "manifest_version": 3,
  "name": "Docs mörker",
  "version": "1.2",
  "description": "Mörkare https://docs.advania.se",
  "permissions": ["storage", "tabs", "scripting"],
  "host_permissions": ["https://docs.advania.se/*"],
  "action": {
    "default_title": "Mörker!",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  }
}
